<?xml version="1.0" encoding="UTF-8"?>
<xsd:schema xmlns:xsd="http://www.w3.org/2001/XMLSchema"
            xmlns="http://www.russianpost.ru/domain/operation-info/2014/09"
            xmlns:common="http://www.russianpost.ru/domain/common/2014/09"
            elementFormDefault="qualified"
            targetNamespace="http://www.russianpost.ru/domain/operation-info/2014/09">
   <xsd:import schemaLocation="common.xsd"
               namespace="http://www.russianpost.ru/domain/common/2014/09"/>
   <xsd:element name="OperationInfo" type="OperationInfo"/>
   <xsd:complexType name="OperationInfo">
      <xsd:sequence>
         <xsd:element name="BaseInfo" type="BaseInfo" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Базовая информация</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="Operands" type="Operands" minOccurs="1" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Операнды</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OperAttrs" type="OperAttrs" minOccurs="0" maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Атрибуты операции над ШИ</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RpoChangedAttrs"
                      type="RpoChangedAttrs"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Изменение РПО</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="BagChangedAttrs"
                      type="BagChangedAttrs"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Изменение емкости</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DocChangedAttrs"
                      type="DocChangedAttrs"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Изменение документа</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="BaseInfo">
      <xsd:sequence>
         <xsd:element name="OperType"
                      type="common:integer-len-4"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код вида операции (см. ПРИЛОЖЕНИЕ 24. Коды операций над РПО, ПРИЛОЖЕНИЕ 25. Коды операций над емкостями, ПРИЛОЖЕНИЕ 26. Коды операций над документами)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OperTypeLocal"
                      type="common:integer-len-4"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код вида операции (Локальный справочник).</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OperTypeNameLocal"
                      type="common:string-len-200"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование вида операции (Локальный справочник).</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OperDateTime"
                      type="common:dateTime"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Дата и время проведения операции (в формате ISO 8601 CCYY-MM-DDThh:mm:ss)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="OperTimeZone"
                      type="common:timeZone"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Часовой пояс даты и времени проведения операции в формате ±hh:mm (по отношению к UTC)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IndexOper"
                      type="common:string-len-6"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Почтовый индекс ОПС места проведения операции</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IndexNext"
                      type="common:string-len-6"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Почтовый индекс ОПС проведения следующей операции</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SysID"
                      type="common:SysIDType"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Идентификатор системы (Уникальный идентификатор системы, участвующей в информационном обмене, см. ПРИЛОЖЕНИЕ 13. Идентификатор информационной системы)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="VersionNumber"
                      type="common:VersionNumberType"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Версия TT, по которой работает система, участвующая в информационном обмене</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="Operands">
      <xsd:sequence>
         <xsd:element name="TypeOfOperand1"
                      type="common:integer-len-2"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Тип операнда 1 - объекта, над которым выполняется операция (см.ПРИЛОЖЕНИЕ 27. Объекты логистической деятельности)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IdOfOperand1"
                      type="common:string-len-100"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Идентификатор операнда 1- объекта, над которым выполняется операция. В случае операции над РПО, емкостью, документом это ШИ соответствующего объекта.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ObjTypeLocId1"
                      type="common:integer-len-4"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Тип операнда 1 - объекта, над которым выполняется операция (Локальный справочник).</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ObjTypeLocName1"
                      type="common:string-len-200"
                      minOccurs="1"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование операнда 1 - объекта, над которым выполняется операция (Локальный справочник).</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TypeOfOperand2"
                      type="common:integer-len-2"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Тип операнда 2 - объекта, к которому осуществляется приписка объекта 1, либо в который осуществляется вложение объекта 1, в результате выполнения операции. (см.ПРИЛОЖЕНИЕ 27. Объекты логистической деятельности).Заполняется в случае выполнения операции приписки (вложения) над РПО, емкостью, документом.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IdOfOperand2"
                      type="common:string-len-100"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Идентификатор операнда 2 – ШИ объекта, к которому осуществляется приписка объекта 1, либо в который осуществляется вложение объекта 1, в результате выполнения операции. Заполняется в случае выполнения операции приписки (вложения) над РПО, емкостью, документом.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ObjTypeLocId2"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Тип операнда 2 - объекта, к которому осуществляется приписка объекта 1, либо в который осуществляется вложение объекта 1, в результате выполнения операции (Локальный справочник). Заполняется в случае выполнения операции приписки (вложения) над РПО, емкостью, документом.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ObjTypeLocName2"
                      type="common:string-len-200"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование операнда 2 - объекта, к которому осуществляется приписка объекта 1, либо в который осуществляется вложение объекта 1, в результате выполнения операции (Локальный справочник). Заполняется в случае выполнения операции приписки (вложения) над РПО, емкостью, документом.</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="OperAttrs">
      <xsd:sequence>
         <xsd:element name="BagBarCode"
                      type="common:string-len-16"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>ШИ емкости (атрибут операции)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DocBarCode"
                      type="common:string-len-16"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>ШИ документа (атрибут операции)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AcceptType"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код вида приема РПО (атрибут операции согласно РТМ 0002- 10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AcceptTypeLocId"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код вида приема РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AcceptTypeLocName"
                      type="common:string-len-20"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование вида приема РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IssueType"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код вида вручени РПО (атрибут операции согласно РТМ 0002- 10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IssueTypeLocId"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код вида вручения РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="IssueTypeLocName"
                      type="common:string-len-20"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование вида вручения РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProcessType"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код вида обработки РПО (атрибут операции согласно РТМ 0002- 10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProcessTypeLocId"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код вида обработки РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="ProcessTypeLocName"
                      type="common:string-len-20"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование вида обработки РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NonDeliv"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код причины невручения РПО (атрибут операции согласно РТМ 0002- 10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NonDelivLocId"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код причины невручения РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="NonDelivLocName"
                      type="common:string-len-20"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование причины невручения РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RetnReason"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код причины возврата РПО (атрибут операции согласно РТМ 0002- 10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RetnReasonLocId"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код причины возврата РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="RetnReasonLocName"
                      type="common:string-len-20"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование причины возврата РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SendAdd"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код причины досылки РПО (атрибут операции согласно РТМ 0002- 10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SendAddLocId"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код причины досылки РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SendAddLocName"
                      type="common:string-len-20"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование причины досылки РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DeposReasn"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код причины хранения РПО (атрибут операции согласно РТМ 0002- 10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DeposReasnLocId"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код причины хранения РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DeposReasnLocName"
                      type="common:string-len-20"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование причины хранения РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DeposTemp"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код причины временного хранения РПО (атрибут операции согласно РТМ 0002- 10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DeposTempLocId"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код причины временного хранения РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="DeposTempLocName"
                      type="common:string-len-20"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование причины временного хранения РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AttemptDeliv"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код причины неудачной попытки вручения РПО (атрибут операции согласно РТМ 0002- 10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AttemptDelivLocId"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код причины неудачной попытки вручения РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="AttemptDelivLocName"
                      type="common:string-len-20"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование причины неудачной попытки вручения РПО (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CustCompl"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код завершения таможенного оформления (согласно РТМ 0002- 10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CustComplLocId"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код завершения таможенного оформления (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="CustComplLocName"
                      type="common:string-len-20"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование завершения таможенного оформления (локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
      </xsd:sequence>
   </xsd:complexType>
   <xsd:complexType name="RpoChangedAttrs">
      <xsd:sequence>
         <xsd:element name="MailType"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код вида отправления (согласно РТМ 0002-10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MailTypeLocal"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код вида отправления (Локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MailTypeNameLocal"
                      type="common:string-len-200"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование вида отправления (Локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MailCtg"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код категории почтового отправления (согласно РТМ 0002-10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MailCtgLocal"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код категории почтового отправления (Локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="MailCtgNameLocal"
                      type="common:string-len-200"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование категории почтового отправления (Локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SendCtg"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код категории отправителя (согласно РТМ 0002- 10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SendCtgLocId"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код категории отправителя (Локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="SendCtgLocName"
                      type="common:string-len-200"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Наименование категории отправителя (Локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransType"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код способа пересылки (согласно РТМ 0002- 10-13)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransTypeLocId"
                      type="common:integer-len-4"
                      minOccurs="0"
                      maxOccurs="1">
            <xsd:annotation>
               <xsd:documentation>Код способа пересылки (Локальный справочник)</xsd:documentation>
            </xsd:annotation>
         </xsd:element>
         <xsd:element name="TransTypeLocName"
                      type="common:string-len-200"
                      minOccurs="0"
