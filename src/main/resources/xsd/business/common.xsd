            <xsd:maxLength value="9" />
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="string-len-10">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="10" />
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="string-len-12">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="12" />
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="string-len-13">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="13" />
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="string-len-14">
        <xsd:restriction base="xsd:string">
            <xsd:minLength value="1" />
            <xsd:maxLength value="14" />
        </xsd:restriction>
    </xsd:simpleType>
    <xsd:simpleType name="string-len-15">
        <xsd:restriction base="xsd:string">
